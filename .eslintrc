{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "indent": [2, 4, {"SwitchCase": 1}],
    "consistent-return": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "react/jsx-boolean-value": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "react/prefer-es6-class": 0,
    "react/no-string-refs": 0,
    "id-length": 0,
    "no-console": 0,
    "no-nested-ternary": 0
  },
  "plugins": [
    "react"
  ]
}

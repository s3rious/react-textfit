{
  "name": "react-textfit",
  "version": "0.1.12",
  "description": "React component to fit headlines and paragraphs into any element",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "lint": "eslint src test examples",
